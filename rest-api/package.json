{
  "name": "rest-api",
  "version": "1.0.0",
  "author": "",
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc",
    "start-server": "node dist/server.js",
    "start-dev-server": "tsc-watch --onSuccess \"node dist/server.js\"",
    "start": "npm-run-all clean build start-server",
    "dev": "npm-run-all clean build start-dev-server",
    "run-populate-db-script": "node dist/model/populate-db.js",
    "populate-db": "npm-run-all clean build run-populate-db-script",
    "run-delete-db-script": "node dist/model/delete-db.js",
    "delete-db": "npm-run-all clean build run-delete-db-script"
  },
  "license": "MIT",
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^18.7.11",
    "express": "^4.18.1",
    "npm-run-all": "^4.1.5",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "tsc-watch": "^5.0.3",
    "typeorm": "^0.3.7",
    "winston": "^3.8.1"
  }
}
